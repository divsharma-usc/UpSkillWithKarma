daml 1.2
module Setup where

import Daml.Script
import HackAdemy.Roles.OperatorRole

initialize : Script ()
initialize = do
  operator <- allocatePartyWithHint "Operator" (PartyIdHint "Operator")
  aliceTV <- submit operator do
    createCmd OperatorRole with
      operator = operator
  pure ()
